#include<bits/stdc++.h>
using namespace std;
struct Node{
  int data;
  Node* left;
  Node* right;
  Node(int data){
      this->data = data;
      this->left = nullptr;
      this->right = nullptr;
  }
};

void preorder(Node* root){
    if(root == nullptr){
        return;
    }
    cout<<root->data<<" ";
    preorder(root->left);
    preorder(root->right);
}
int main(){
    Node* root = new Node(1);
    root->left = new Node(2);
    root->right = new Node(3);// 1->root, 2->left, 3-> right
    //root->left->left = new Node(4);
    //root->left->right = new Node(5);
//           1
//       / \
//      2   3
//     / \
//   4   5

    
    preorder(root);
}
